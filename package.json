{
  "name": "veil-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=@veil/web",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "test": "turbo run test"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "prisma": "5.22.0",
    "turbo": "latest",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@8.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "5.22.0",
    "zod": "^4.1.13"
  }
}